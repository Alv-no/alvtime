{
  "name": "Alvtime-slack-app",
  "version": "0.0.1",
  "description": "Alvtime slack app",
  "main": "app.js",
  "scripts": {
    "start": "npm run serve",
    "build": "tsc",
    "serve": "node dist/app.js",
    "watch-node": "nodemon dist/app.js | pino-pretty -t",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "watch-ts": "tsc -w",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect-brk=0.0.0.0 dist/app.js | pino-pretty -t",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "dependencies": {
    "@slack/bolt": "^3.9.0",
    "@slack/events-api": "^3.0.1",
    "@slack/interactive-messages": "^2.0.2",
    "@slack/web-api": "^6.6.0",
    "body-parser": "^1.19.1",
    "date-easter": "^0.2.5",
    "date-fns": "^2.28.0",
    "dotenv": "^8.6.0",
    "express": "^4.17.3",
    "express-session": "^1.17.2",
    "fuse.js": "^6.5.3",
    "jwt-simple": "^0.5.6",
    "moment": "^2.29.2",
    "mongoose": "^5.13.13",
    "mongoose-field-encryption": "3.0.4",
    "node-cron": "^2.0.3",
    "node-fetch": "^2.6.7",
    "passport": "^0.4.1",
    "passport-oauth2": "^1.6.1",
    "pino": "^6.13.3",
    "pino-http": "^5.8.0",
    "uuid": "^8.3.2"
  },
  "repository": {
    "url": "https://github.com/Alv-no/alvtime-slack-app"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "slack",
    "express"
  ],
  "devDependencies": {
    "@types/concurrently": "^5.2.1",
    "@types/eslint": "^8.2.1",
    "@types/express-session": "^1.17.4",
    "@types/jest": "^27.0.3",
    "@types/node": "16.11.13",
    "@types/node-cron": "^2.0.3",
    "@types/node-fetch": "^2.5.12",
    "@types/passport": "^1.0.3",
    "@types/passport-oauth2": "^1.4.9",
    "@types/pino": "^6.3.12",
    "@types/pino-http": "^5.8.1",
    "@types/uuid": "^7.0.5",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "concurrently": "^6.4.0",
    "eslint": "^8.4.1",
    "jest": "^27.4.5",
    "nodemon": "^2.0.15",
    "pino-pretty": "^7.5.1",
    "ts-jest": "^27.1.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
